{
  "name": "@visa/nova-react-workshop",
  "version": "3.0.0",
  "description": "VISA Product Design System Nova React Library",
  "homepage": "/react",
  "private": true,
  "type": "module",
  "dependencies": {
    "@visa/nova-icons-react": "^1 || ^2 || ^3",
    "@visa/nova-react": "workspace:*",
    "@visa/nova-styles": "^1.6.4",
    "clsx": "^2.1.1",
    "react": "^19",
    "react-dom": "^19"
  },
  "optionalDependencies": {
    "@floating-ui/react": "^0.27.2",
    "downshift": "^9.0.4"
  },
  "scripts": {
    "analyze": "pnpx vite-bundle-visualizer",
    "build": "tsc && vite build",
    "dev": "vite --open",
    "prebuild": "cp ../../CHANGELOG.md ./public/CHANGELOG.md && pnpm statsGen",
    "preview": "vite preview",
    "statsGen": "node ./bin/stat-gen.mjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "use:registry": "npm install --ignore-scripts @visa/nova-react@latest",
    "use:workspace": "node -e \"const fs=require('fs');const pkg=JSON.parse(fs.readFileSync('package.json','utf8'));pkg.dependencies['@visa/nova-react']='workspace:*';fs.writeFileSync('package.json',JSON.stringify(pkg,null,2));\"",
    "watch": "vite"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visa/nova-react.git"
  },
  "keywords": [
    "design system",
    "JS",
    "nova",
    "react",
    "rollup",
    "typescript",
    "UI",
    "vds",
    "visa",
    "visa-ui",
    "vite"
  ],
  "license": "Visa Proprietary",
  "devDependencies": {
    "@tanstack/react-query": "^5.35.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^19.x",
    "@types/react-dom": "^19.x",
    "@vitejs/plugin-react": "^5.0.0",
    "change-case": "^5.0.0",
    "gitlog": "^5.0.2",
    "glob": "^10.3.14",
    "jest": "^29.7.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "prism-react-renderer": "^2.3.1",
    "react-ga4": "^2.1.0",
    "react-markdown": "^10.0.0",
    "react-router-dom": "^6.28.1",
    "sass": "^1.77.0",
    "ts-jest": "^29.4.0",
    "ts-jest-mock-import-meta": "^1.3.1",
    "vite": "^7.1.5",
    "vite-bundle-visualizer": "^1.1.0",
    "vite-plugin-svgr": "^4.2.0"
  }
}
